{
    "name": "naowas/crud-generator",
    "description": "Simple Laravel Crud Generator",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Naowas\\CrudGenerator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Naowas Morshed Eimon",
            "email": "naowas.morshed.eimon@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "laravel/framework": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naowas\\CrudGenerator\\CrudGeneratorServiceProvider"
            ],
            "aliases": {
                "CrudGenerator": "Naowas\\CrudGenerator\\Facades\\CrudGenerator"
            }
        }
    },
    "minimum-stability": "dev"
}
